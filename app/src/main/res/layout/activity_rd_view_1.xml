<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="rdView1VM" type="com.utkarshsapplication.app.modules.rdview1.data.viewmodel.RdView1VM"/>
    </data>
    <FrameLayout android:id="@+id/frame3rdview1"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_300">
        <LinearLayout android:id="@+id/linearColumnlanguage"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStacklanguage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_394pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_4pxh" android:layout_width="@dimen/_300pxh">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtLanguage"
                    android:layout_gravity="top|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_width="match_parent"
                    android:text="@{rdView1VM.rdView1Model.txtLanguage}"
                    style="@style/txtInterbold30" tools:text="@string/msg_welcome_to_mil"/>
                <ImageView android:id="@+id/imageWhatsAppImage"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_332pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginEnd="@dimen/_43pxh"
                    android:layout_marginStart="@dimen/_43pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_214pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_whatsappimage_332x214}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_whatsappimage_332x214"/>
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtGroupFour"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@{rdView1VM.rdView1Model.txtGroupFour}"
                    style="@style/txtSolidRounded" tools:text="@string/msg_sign_in_sign_u"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtGroupThree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_7pxv"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_2pxv"
                android:singleLine="true"
                android:text="@{rdView1VM.rdView1Model.txtGroupThree}"
                style="@style/txtSolidRoundedOutline" tools:text="@string/msg_continue_with_e"/>
            <LinearLayout android:id="@+id/linearRowfbremovebgpre"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackfbremovebgpre"
                    android:layout_height="@dimen/_30pxv"
                    android:layout_width="@dimen/_135pxh" style="@style/groupStylegray_300cornerRadius">
                    <ImageView android:id="@+id/imageFbremovebgpre"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_fbremovebgpre"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_fbremovebgpre"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStacktwitterremoveb"
                    android:layout_height="@dimen/_30pxv"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_width="@dimen/_135pxh" style="@style/groupStylegray_300cornerRadius">
                    <ImageView android:id="@+id/imageTwitterremoveb"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginEnd="@dimen/_51pxh"
                        android:layout_marginStart="@dimen/_51pxh"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_twitterremoveb"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_twitterremoveb"/>
                </FrameLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStacktermandcondition"
                android:layout_height="@dimen/_32pxv"
                android:layout_marginTop="@dimen/_32pxv" android:layout_width="@dimen/_317pxh">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtTermAndCondition"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_width="match_parent"
                    android:text="@{rdView1VM.rdView1Model.txtTermAndCondition}"
                    style="@style/txtInterlight12" tools:text="@string/msg_by_continuing_y"/>
                <View android:background="@color/black_900"
                    android:id="@+id/lineLineTwo"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_130pxh"/>
            </FrameLayout>
            <View android:background="@color/black_900"
                android:id="@+id/lineLineOne"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_80pxh"/>
        </LinearLayout>
    </FrameLayout>
</layout>
