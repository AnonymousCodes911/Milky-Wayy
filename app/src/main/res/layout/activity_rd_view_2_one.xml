<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="rdView2OneVM" type="com.utkarshsapplication.app.modules.rdview2one.data.viewmodel.RdView2OneVM"/>
    </data>
    <LinearLayout android:id="@+id/linear3rdview2One"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_300">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowOne"
                    android:layout_height="@dimen/_2pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow1_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow1_black_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLogin"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_235pxh"
                    android:layout_weight="0.68"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{rdView2OneVM.rdView2OneModel.txtLogin}"
                    style="@style/txtIntermedium18" tools:text="@string/lbl_log_in"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <TextView android:ellipsize="end" android:id="@+id/txtSignUp"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_42pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{rdView2OneVM.rdView2OneModel.txtSignUp}"
            style="@style/txtInterbold25" tools:text="@string/lbl_sign_up"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtCreateyournew"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_42pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{rdView2OneVM.rdView2OneModel.txtCreateyournew}"
            style="@style/txtInterextralight14" tools:text="@string/msg_create_your_new"/>
        <TextView android:ellipsize="end" android:id="@+id/txtName"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_42pxh"
            android:layout_marginTop="@dimen/_29pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{rdView2OneVM.rdView2OneModel.txtName}"
            style="@style/txtInterextralight14" tools:text="@string/lbl_name"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtAkankshaGarg"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_42pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{rdView2OneVM.rdView2OneModel.txtAkankshaGarg}"
            style="@style/txtInterregular14" tools:text="Your Name"/>
        <View android:background="@color/black_900_4c"
            android:id="@+id/lineLineThree"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_277pxh"/>
        <View android:background="@color/black_900_4c"
            android:id="@+id/lineLineFour"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh" android:layout_width="@dimen/_277pxh"/>

        <TextView
            android:id="@+id/txtEmail"
            style="@style/txtInterextralight14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginStart="@dimen/_44pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_marginEnd="@dimen/_44pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@{rdView2OneVM.rdView2OneModel.txtEmail}"
            tools:text="@string/lbl_email" />

        <EditText android:ellipsize="end"
            android:hint="@string/msg_akankshagarg301"
            android:id="@+id/etEmailOne"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_7pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_3pxh"
            android:singleLine="true"
            android:text="@={rdView2OneVM.rdView2OneModel.etEmailOneValue}"
            android:textColorHint="@color/black_900"
            style="@style/etInterregular14" tools:ignore="Autofill"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPassword"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_42pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{rdView2OneVM.rdView2OneModel.txtPassword}"
            style="@style/txtInterextralight14" tools:text="@string/lbl_password"/>
        <LinearLayout android:id="@+id/linearRowpasswordOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtPasswordOne"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.79" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{rdView2OneVM.rdView2OneModel.txtPasswordOne}"
                style="@style/txtInterregular14" tools:text="@string/lbl"/>
            <ImageView android:id="@+id/imagePasswordOne"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_160pxh"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_password1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_password1"/>
        </LinearLayout>
        <View android:background="@color/black_900_4c"
            android:id="@+id/lineLineSix"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_277pxh"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumncontinue"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_79pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnContinue"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true"
                android:text="@string/lbl_continue"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_continue"/>
            <LinearLayout android:id="@+id/linearRowlineeight"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View android:background="@color/black_900_4c"
                    android:id="@+id/lineLineEight"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.3" android:layout_width="0dp"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOrcontinuewit"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_weight="0.39"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{rdView2OneVM.rdView2OneModel.txtOrcontinuewit}"
                    style="@style/txtInterextralight14" tools:text="@string/msg_or_continue_wit"/>
                <View android:background="@color/black_900_4c"
                    android:id="@+id/lineLineSeven"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.32" android:layout_width="0dp"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowfbremovebgpre"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackfbremovebgpre"
                    android:layout_height="@dimen/_30pxv"
                    android:layout_width="@dimen/_135pxh" style="@style/groupStylegray_300cornerRadius">
                    <ImageView android:id="@+id/imageFbremovebgpre"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_fbremovebgpre"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_fbremovebgpre"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStacktwitterremoveb"
                    android:layout_height="@dimen/_30pxv"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_width="@dimen/_135pxh" style="@style/groupStylegray_300cornerRadius">
                    <ImageView android:id="@+id/imageTwitterremoveb"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_twitterremoveb"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_twitterremoveb"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
